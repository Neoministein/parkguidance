<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

	<servlet>
		<servlet-name>ParkDataReceiver</servlet-name>
		<servlet-class>com.neo.parkguidance.parkdata.receiver.api.servlet.ParkingService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ParkDataReceiver</servlet-name>
		<url-pattern>park-data/receiver</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ParkDataSorter</servlet-name>
		<servlet-class>com.neo.parkguidance.parkdata.sorter.api.DataSorterService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ParkDataSorter</servlet-name>
		<url-pattern>park-data/sorter</url-pattern>
	</servlet-mapping>

	<filter>
		<filter-name>InternalCall</filter-name>
		<filter-class>com.neo.parkguidance.core.api.internal.InternalFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>InternalCall</filter-name>
		<url-pattern>/park-data/sorter</url-pattern>
	</filter-mapping>
</web-app>
